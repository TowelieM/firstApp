              "main": "src/main.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "tsconfig.json",
              "aot": true
            }
          },
          "serve": {
            "builder": "@angular-devkit/build-angular:dev-server",
            "options": {
              "browserTarget": "purchaseapp:build"
            }
          }
        }
      }},
    "defaultProject": "purchaseapp",
    "cli": {
      "analytics": "780c95ac-c59a-43ec-ae23-a5049ddcb82a"
    }
